<h1>Playlists#index</h1>

<table class="table table-striped text-left">
  <thead>
    <tr>
      <th scope="col">Playlist Name</th>
      <!--<th scope="col">Description</th>-->
    </tr>
  </thead>
  <tbody>
    <% @playlists.each do |playlist| %>
        <tr>
        <td><%= link_to "#{playlist.name}", playlist_path(playlist) %></td>
        </tr>
    <% end %>
  </tbody>
</table>



